{
	"devDependencies": {
		"@taplo/cli": "^0.3.2",
		"@types/node": "^14.14.41",
		"cross-env": "^7.0.3",
		"cspell": "^5.12.3",
		"lint-staged": "^12.3.6",
		"npm-run-all": "^4.1.5"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/swc"
	},
	"lint-staged": {
		"!(**/tests/**/*)*.js": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.jsx": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.ts": [
			"prettier --write"
		],
		"!(**/tests/**/*)*.tsx": [
			"prettier --write"
		],
		"*.rs": [
			"rustfmt --"
		],
		"*.toml": [
			"taplo format"
		]
	},
	"main": "index.js",
	"name": "@swc/bindings",
	"scripts": {
		"prepare": "husky install"
	},
	"workspaces": [
		"packages/*"
	]
}
